{
  "name": "@oz-adv/backend",
  "version": "0.0.1",
  "description": "Backend for the Oz-Adv project",
  "type": "module",
  "scripts": {
    "start": "node dist/index.mjs",
    "build": "rm -rf ./dist && rollup -c",
    "watch": "rollup -cw",
    "dev": "tsx --env-file=.env --watch src/index.mts",
    "lint": "eslint \"**/{src,test,examples,scripts,dtslint}/**/*.{ts,mjs}\"",
    "lint-fix": "pnpm lint --fix",
    "test": "vitest"
  },
  "dependencies": {
    "@effect/experimental": "^0.30.7",
    "@effect/opentelemetry": "^0.39.2",
    "@effect/platform": "^0.69.6",
    "@effect/platform-node": "^0.64.7",
    "@effect/schema": "^0.75.5",
    "@effect/sql": "^0.18.7",
    "@effect/sql-pg": "^0.18.7",
    "effect": "^3.10.2",
    "undici": "^6.20.1"
  },
  "devDependencies": {
    "@effect/language-service": "^0.2.0",
    "@effect/vitest": "^0.13.2",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.1",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-swc": "^0.4.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@swc-node/core": "^1.13.3",
    "@swc/cli": "0.4.1-nightly.20240914",
    "@swc/core": "^1.7.39",
    "@types/node": "^22.7.9",
    "@types/uuid": "^10.0.0",
    "rollup": "^4.24.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.3",
    "vitest": "^2.1.3"
  },
  "pnpm": {
    "patchedDependencies": {
      "postgres@3.4.4": "patches/postgres@3.4.4.patch"
    }
  }
}
